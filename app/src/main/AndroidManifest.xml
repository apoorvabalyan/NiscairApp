<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apoorva.niscair">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label = "Training Portal"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.apoorva.niscair.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".homeScreen"
            android:windowSoftInputMode="adjustNothing|stateHidden" />
        <activity android:name=".about" />
        <activity android:name=".contact" />
        <activity
            android:name=".course"
            android:windowSoftInputMode="adjustNothing|stateHidden" />
        <activity
            android:name=".newuser"
            android:windowSoftInputMode="adjustNothing|stateHidden" />
        <activity
            android:name=".userhome"
            android:windowSoftInputMode="adjustNothing|stateHidden" />
        <activity
            android:name=".alreadyregister"
            android:windowSoftInputMode="adjustNothing|stateHidden" />
        <activity
            android:name=".forget"
            android:windowSoftInputMode="adjustNothing|stateHidden" />
        <activity
            android:name=".form"
            android:windowSoftInputMode="adjustNothing|stateHidden"></activity>
        <activity
            android:name=".payment"
            android:windowSoftInputMode="adjustNothing|stateHidden"></activity>
    </application>

</manifest>